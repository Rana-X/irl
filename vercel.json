{
  "functions": {
    "api/mcp.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    { "source": "/api/mcp", "destination": "/api/mcp.js" },
    { "source": "/favicon.ico", "destination": "/public/irl-logo.svg" },
    { "source": "/", "destination": "/public/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}